<tr>
  <td><%= thecase.title %></td>
  <td><%= thecase.level %></td>
  <td><%= case_parent_for(thecase.parent) %></td>
  <td>
    <%= link_to '', [:edit, :cpanel, thecase], class:'cm-edit', title: '点击编辑' %> |
    <%= link_to '', [:cpanel, thecase], method: :delete, data: { confirm: '确认删除' }, class: 'cm-close', title: '点击删除' %>
  </td>
</tr>
<%= render partial: 'case', collection: thecase.subcases, as: :thecase %>
